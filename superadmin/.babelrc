{
  "presets": [ "es2015", "react", "stage-0" ],
  "plugins": [
    "transform-runtime",
    "transform-class-properties",
    "syntax-class-properties",
    "react-hot-loader/babel",
    ["babel-root-import", [{
      "rootPathPrefix": "~",
      "rootPathSuffix": "app/"
    }, {
      "rootPathPrefix": "@",
      "rootPathSuffix": "app/shared/"
    }]],
    "./app/plugins/babelRelayPlugin"
  ]
}
